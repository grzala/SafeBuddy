window.addEventListener('message', onMessage);
function onMessage(e) {
var message;
try {
message = JSON.parse(e.data);
} catch (ignore) {
return;
}
if (e.origin.indexOf('tinypass.com') !== -1) {
switch (message.command) {
case 'openPopup':
window.tp.push(['init', function(){
tp.template.show({
templateId: message.popupId,
displayMode: 'modal'
});
}]);
break;
case 'openURL':
window.location.href = message.url;
break;
default:
return;
} }
}
</script>
